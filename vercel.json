{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "NORMECA-angular/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/NORMECA-angular/(.*)",
      "dest": "/NORMECA-angular/$1"  
    },
    {
      "src": "/(.*)",
      "dest": "/NORMECA-angular/index.html"  
    }
  ]
}
